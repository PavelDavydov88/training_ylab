<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">

    <changeSet id="005-create-table-transaction" author="pavel">
        <createSequence sequenceName="SEQUENCE_TRANSACTION" incrementBy="1" cacheSize="1" maxValue="9223372036854775807" startValue="40" schemaName="wallet"/>
        <createTable tableName="transaction" schemaName="wallet">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false" unique="true"/>
            </column>
            <column name="name_transaction" type="VARCHAR(255)">
                <constraints nullable="false" unique="true"/>
            </column>
        </createTable>

    </changeSet>
</databaseChangeLog>